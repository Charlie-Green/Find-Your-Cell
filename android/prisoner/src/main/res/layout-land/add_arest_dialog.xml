<?xml version="1.0" encoding="utf-8"?>
<!-- Dialog dimensions are set programmatically. -->
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.savvi.rangedatepicker.CalendarPickerView
        android:id="@+id/calpicker"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:tsquare_headerTextColor="?colorPrimary"
        app:tsquare_titleTextColor="?colorPrimary"
        app:tsquare_dividerColor="?android:textColorSecondary"
        app:tsquare_orientation_horizontal="true" />

</FrameLayout>